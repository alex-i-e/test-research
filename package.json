{
  "name": "image-research",
  "private": true,
  "version": "0.0.1",
  "description": "Research images",
  "main": "index.html",
  "repository": "git@github.com:alex-i-e/test-research.git",
  "license": "MIT",
  "type": "module",
  "scripts": {
